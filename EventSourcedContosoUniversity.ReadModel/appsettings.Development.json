{
  "Serilog": {
        "Using": [ "Serilog.Sinks.Console" ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "RollingFile",
                "Args": { "pathFormat": "%tmp%/esweb-log-{Date}.txt" }
            }
        ],
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
        "Properties": {
            "Application": "EventSourcedContosoUniversityWeb"
        }
    },
}
